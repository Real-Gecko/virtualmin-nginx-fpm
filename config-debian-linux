nginx_config=/etc/nginx/nginx.conf
nginx_cmd=/usr/sbin/nginx
nginx_start_cmd=service nginx start
nginx_stop_cmd=service nginx stop
nginx_reload_cmd=service nginx restart
nginx_sites_available=/etc/nginx/sites-available
nginx_sites_enabled=/etc/nginx/sites-enabled
nginx_user=www-data
fpm_cmd=/usr/sbin/php5-fpm
fpm_start=service php5-fpm start
fpm_stop=service php5-fpm stop
fpm_reload=service php5-fpm restart
fpm_poold=/etc/php5/fpm/pool.d
