nginx_config=/etc/nginx/nginx.conf
nginx_cmd=/usr/sbin/nginx
nginx_start_cmd=service nginx start
nginx_stop_cmd=service nginx stop
nginx_reload_cmd=service nginx restart
nginx_sites_available=/etc/nginx/conf.d
nginx_user=nginx
fpm_cmd=/usr/sbin/php-fpm
fpm_start=service php-fpm start
fpm_stop=service php-fpm stop
fpm_reload=service php-fpm restart
fpm_poold=/etc/php-fpm.d
